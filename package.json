{
  "name": "yapi-vendor",
  "version": "1.10.2",
  "description": "YAPI",
  "scripts": {
    "lint": "prettier --write .",
    "dev-copy-icon": "cp -r static/iconfont ./",
    "start": "nodemon server/app.js dev -L",
    "install-server": " node server/install.js",
    "preview": "cross-env NODE_ENV=production node server/app.js",
    "test": "ava",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "deploy": "ghca deploy -c ./privateConfigs/server.js",
    "deploy:c": "ghca deploy -c ./privateConfigs/client.js",
    "docs": "ydoc build"
  },
  "scripts-info": {
    "start": "运行生产环境服务器",
    "install-server": "初始化数据库数据，用于安装",
    "dev": "运行开发服务器",
    "test": "执行测试"
  },
  "build": {
    "appId": "Shone.Sing.Lone.Ventose",
    "files": [
      "**/*"
    ],
    "extraFiles": [
      ".git",
      ".github",
      ".idea",
      "dist-exec",
      "test"
    ],
    "productName": "Ventose",
    "copyright": "Copyright © 2023 Shone",
    "directories": {
      "buildResources": "build",
      "output": "../dist-exec"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "language": "2052",
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:YMFE/yapi.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "ajv": "^5.5.1",
    "ajv-i18n": "^2.2.0",
    "ava": "^0.22.0",
    "axios": "1.2.0",
    "compare-versions": "^3.4.0",
    "copy-to-clipboard": "3.0.8",
    "cpu-load": "^1.0.0",
    "crypto-js": "^3.1.9-1",
    "deep-extend": "^0.5.0",
    "deref": "^0.7.3",
    "easy-json-schema": "0.0.2-beta",
    "electron-cookies": "^1.1.0",
    "eslint": "^3.19.0",
    "fs-extra": "^3.0.1",
    "generate-schema": "^2.6.0",
    "ghooks": "^2.0.0",
    "immer": "^1.1.1",
    "js-base64": "^2.3.2",
    "json-schema-faker": "0.5.0-rc16",
    "json-schema-ref-parser": "4.0.0",
    "json5": "0.5.1",
    "jsondiffpatch": "0.3.11",
    "jsonwebtoken": "7.4.1",
    "jsrsasign": "^8.0.12",
    "koa": "2.0.0",
    "koa-body": "^2.5.0",
    "koa-bodyparser": "3.2.0",
    "koa-multer": "1.0.2",
    "koa-mysql-session": "0.0.2",
    "koa-router": "^7.0.1",
    "koa-send": "3.2.0",
    "koa-session-minimal": "3.0.3",
    "koa-static": "3.0.0",
    "koa-websocket": "4.0.0",
    "koa2-cors": "^2.0.6",
    "ldapjs": "^1.0.1",
    "lodash": "^4.17.21",
    "markdown-it": "8.4.0",
    "markdown-it-anchor": "4.0.0",
    "markdown-it-table-of-contents": "0.3.2",
    "md5": "2.2.1",
    "mockjs": "1.0.1-beta3",
    "moment": "^2.19.3",
    "mongodb": "3.1.8",
    "mongoose": "5.7.5",
    "mongoose-auto-increment": "5.0.1",
    "node-schedule": "^1.3.2",
    "nodemailer": "4.0.1",
    "nodemon": "^1.11.0",
    "os": "0.1.1",
    "prettier": "^2.2.1",
    "qs": "^6.7.0",
    "rewire": "^2.5.2",
    "safeify": "^5.0.5",
    "sha.js": "2.4.9",
    "sha1": "1.1.1",
    "swagger-client": "^3.5.1",
    "tslib": "1.8.0",
    "underscore": "1.13.6",
    "url": "0.11.0",
    "validate-commit-msg": "^2.12.2",
    "vm2": "^3.8.4",
    "yapi-plugin-qsso": "^1.1.0"
  },
  "engines": {
    "node": ">= 7.6.0",
    "npm": ">= 4.1.2"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}